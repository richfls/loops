#include <iostream>
using namespace std;
int main() {
	int shuffle;
	int lol=0;

	for (int i = 0; i < 70; i += 10) {
		cout << i <<" ";
	}
	cout<<endl;
	for (int i = 20; i > -20; i--) {
		cout << i << " ";
	}
	for (int i = 3; i < 2000; i*=5) {
		cout << i << " ";
	}
	cout << endl;
	for (int i = 0; i <=12; i++) {
		for (int i = 0; i <=8; i++) {
			cout << '*' << " ";
		}
		cout << endl;
	}
	cout << "how many times would you like to shuffle" << endl;
	cin >> shuffle;

	while (lol < shuffle) {
		lol+=1;
		cout <<"shuffle"<< endl;
	}




}
